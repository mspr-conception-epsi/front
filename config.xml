<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.MSPR-FRONT" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MSPR-FRONT</name>
    <description>
        Cordova/VueJs application for EPSI's MSPR
    </description>
    <author email="fassot.clement@gmail.com" href="http://github.com/ked57">
        Cl√©ment Fassot
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="phonegap-version" value="cli-8.1.1" />
    <preference name="GOOGLE_MAPS_ANDROID_API_KEY" value="AIzaSyBQO57nsBOmrvoz6AF4Wb9FJ5m4S3vWfuI" />
</widget>
